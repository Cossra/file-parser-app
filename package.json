    "@opentelemetry/winston-transport": "^0.14.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@schematichq/schematic-components": "^1.1.3",
    "@schematichq/schematic-react": "^1.2.5",
    "@schematichq/schematic-typescript-node": "^1.1.12",
    "@stripe/react-stripe-js": "^3.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "1.26.0-alpha.8",
    "lucide-react": "^0.537.0",
    "next": "15.2.3",
    "openai": "^5.12.2",
    "pdf-parse": "^1.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.6"
