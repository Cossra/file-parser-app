  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "inngest": "^3.40.1",
    "inngest-cli": "^1.11.5",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "postcss": "^8.5.6",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.5.3",
    "tailwindcss": "3.4.3",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@tailwindcss/oxide",
      "esbuild",
      "inngest-cli",
      "postinstall-postinstall",
      "protobufjs",
      "sharp",
      "unrs-resolver"
    ],
    "overrides": {
      "zod": "3.24.1"
    }
  }
}
